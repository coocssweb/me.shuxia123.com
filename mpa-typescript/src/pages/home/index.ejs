        <div class="viewer">
            <svg width="100%" height="300px">
                <defs>
                    <filter id="GaussianBlur">
                        <feGaussianBlur in="SourceGraphic" stdDeviation="60" />
                    </filter>
                </defs>
                <image x="0" y="0" width="100%" height="100%" externalResourcesRequired="true" xlink:href="https://www.shuxia123.com/images/personal.jpg" style="filter:url(#GaussianBlur)" preserveAspectRatio="none"></image>
            </svg>
            <div class="viewer-avatar">
                <img src="https://www.shuxia123.com/images/personal.jpg" />
            </div>
        </div>
    </div>
</section>

